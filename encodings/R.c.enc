@@PTR_TYPE
int

@@INIT
p_g = HAVOC_INT;
last = HAVOC_HEAP;

@@GLOBAL_DECLS
unsigned int cnt = 0;
int allocCtr = 0;
int p_g;
HEAP_TYPE last;
void free(int p){}

@@PREDICATE
R(int r_cnt, HEAP_TYPE n)

@@READ_FN
HEAP_TYPE read(PTR_TYPE p) {
  HEAP_TYPE result;
  ++cnt;
  if (p_g == p) {
    assert(R(cnt, last));
    result = last;
  } else {
    result = HAVOC_HEAP;
    assume(R(cnt, result));
  }
  return result;
}

@@WRITE_FN
void write(PTR_TYPE p, HEAP_TYPE e) {
  if (p_g == p && 0 < p && p <= allocCtr)
    last = e;
}

@@ALLOC_FN
int alloc() {
  int p = ++allocCtr;
  if(p_g == p) {
    last = HAVOC_HEAP;
  }
  return p;
}